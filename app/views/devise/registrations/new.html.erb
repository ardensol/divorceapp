
<div class="loginwrap">
  <div class="login-head">
  <h2>Sign Up</h2>
  </div>
  <div class="signup-form">
    <% if params[:vendor] %>
      <%= render 'vendor_form' %>
    <% else %>
      <%= render 'regular_form' %>
    <% end %>
    <div class="login-footer">
      <div class="right">
        Already have an account? <%= render "devise/shared/links" %>
      </div>
    </div>
  
</div>